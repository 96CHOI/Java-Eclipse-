package cafe.DAO;

import java.sql.SQLException;

public class food_DAO extends base_DAO {
	
	public void food_list() {
		
		String sql = "select * from food";
		
		try {
			pt = conn.prepareStatement(sql);
			rs = pt.executeQuery();
			while(rs.next()) {// 여러 레코드를 조회 했다면 반복문 처리
				food data = new food();
			}
			
		}catch(SQLException e) {
			e.printStackTrace();
		}
	}
}
