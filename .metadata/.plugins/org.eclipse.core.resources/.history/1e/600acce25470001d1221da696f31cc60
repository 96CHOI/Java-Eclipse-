package command;

import java.util.List;

import DAO.venture_DAO;
import VO.venture;

public class main_cmd {
	public static venture_DAO vdao = new venture_DAO();
	
	public static void main(String[] args) {
		List<venture> list = vdao.Select_all();
		
		for(venture v : list) {
			System.out.println("번호 : " + v.getNum()+"\n회사이름 : "+v.getCompany()+" : "+v.getAddr()+""
							+v.getCategory()+""+v.getBusiness_name()+""+v.getProduct());
		}
	}
}
