package DAO;

import java.sql.SQLException;
import java.util.ArrayList;
import java.util.List;

public class venture_DAO extends init_DAO {

	public List<venture> Select_all(){
		List<venture> list = new ArrayList<>();
		
		String sql = "select * from venture";
		
		try {
			pt = conn.prepareStatement(sql);
			rs = pt.executeQuery();
			while (rs.next()) {
				venture data = new venture(rs.getInt("num"),rs.getString("company"),
						rs.getString("addr"), rs.getString("catrgory"),rs.getString("name"),
						rs.getString("product"));
			}
			
		}catch (SQLException e) {
			e.printStackTrace();
	}
}
